{
  "presets": ["env", "react"],
  "plugins": [
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ],
    "transform-object-rest-spread",
    "babel-plugin-transform-decorators-legacy",
    ["transform-class-properties", { "loose": true }]
  ]
}
